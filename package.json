{
  "name": "game",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "fix": "prettier --write .",
    "lint": "prettier --check .",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:integration": "npx playwright test --ui",
    "test:integration:ci": "npx playwright test",
    "test:integration:click-create": "npx playwright codegen",
    "test:e2e": "BASE_URL=${BASE_URL:-https://reactle.vercel.app} npm run test:integration",
    "test:e2e:ci": "BASE_URL=${BASE_URL:-https://reactle.vercel.app} npm run test:integration:ci",
    "prepare": "husky install",
    "storybook": "npm list storybook || npm install storybook@7.6.17 --no-save && storybook dev -p 6006",
    "build-storybook": "npm list storybook || npm install storybook@7.6.17 --no-save && storybook build"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^2.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.11.16",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/ua-parser-js": "^0.7.39",
    "classnames": "^2.5.1",
    "date-fns": "^3.3.1",
    "egoroof-blowfish": "^2.2.2",
    "grapheme-splitter": "1.0.4",
    "query-string": "^9.0.0",
    "react": "^17.0.2",
    "react-countdown": "^2.3.5",
    "react-datepicker": "^4.8.0",
    "react-div-100vh": "^0.7.0",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.5.4",
    "ua-parser-js": "^1.0.37",
    "web-vitals": "^3.4.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.42.0",
    "@storybook/preset-create-react-app": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-webpack5": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react-datepicker": "^4.4.2",
    "autoprefixer": "^10.4.17",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^9.0.11",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.5",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.4.1",
    "webpack": "^5.89.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,css,md}": "prettier --write"
  },
  "jest": {
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/src/$1"
    }
  }
}
